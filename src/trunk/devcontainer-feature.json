{
  "name": "Trunk",
  "id": "trunk",
  "version": "1.1.0",
  "description": "The Trunk CLI tool and extension (learn more at trunk.io)",
  "customizations": {
    "vscode": {
      "extensions": ["trunk.io"],
      "settings": {
        "files.exclude": {
          "**/.trunk/*actions/": true,
          "**/.trunk/*logs/": true,
          "**/.trunk/*notifications/": true,
          "**/.trunk/*out/": true,
          "**/.trunk/*plugins/": true,
          "**/.trunk/*tools/": true
        },
        "files.watcherExclude": {
          "**/.trunk/*actions/": true,
          "**/.trunk/*logs/": true,
          "**/.trunk/*notifications/": true,
          "**/.trunk/*out/": true,
          "**/.trunk/*plugins/": true,
          "**/.trunk/*tools/": true
        }
      }
    }
  },
  "installsAfter": ["ghcr.io/devcontainers/features/common-utils"]
}
